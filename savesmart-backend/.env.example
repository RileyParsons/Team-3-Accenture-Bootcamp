# Server Configuration
PORT=3001
NODE_ENV=development

# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# DynamoDB Table Names
DYNAMODB_USERS_TABLE=savesmart-users
DYNAMODB_PLANS_TABLE=savesmart-plans
DYNAMODB_EVENTS_TABLE=savesmart-events
DYNAMODB_RECIPES_TABLE=savesmart-recipes
DYNAMODB_FUEL_STATIONS_TABLE=savesmart-fuel-stations
DYNAMODB_TRANSACTIONS_TABLE=savesmart-transactions

# OpenAI API Configuration (replacing n8n webhooks temporarily)
OPENAI_API_KEY=your_openai_api_key_here

# External API Keys (Optional - will use mock data if not provided)
# Eventbrite API Key - Optional OAuth Bearer token for Eventbrite API v3
# If not set, the application will automatically fall back to mock event data
# Format: Private OAuth token from https://www.eventbrite.com/platform/api#/introduction/authentication
# Example: EVENTBRITE_API_KEY=ABC123XYZ456DEF789GHI012JKL345MNO678PQR901STU234VWX567YZ
EVENTBRITE_API_KEY=TWB3FUWVYMZ2KARVWKJ2
FUELCHECK_API_KEY=
GROCERY_API_KEY=

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
